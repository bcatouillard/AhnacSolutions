	oFile.Close
	Set oFile = Nothing
	Set oFso = Nothing
	sHtml = sHtml & vbCrLf & _ 
		"===== Utilisateur : " & sUtilisateur & " =====" & vbCrLf & vbCrLf & _ 
		"* Type d'incident : " & sTypeReduit & vbCrLf & _ 
		"  =-=-=--=-=-=-=-=" & vbCrLf & _ 
		"* Contexte de l'incident :" & vbCrLf & _ 
		"  =-=-=-=-=-=-=-=-=-=-=-=" & vbCrLf & _ 
