	Dim nRep
	Dim sFrom, sTo, sCc, sBcc
	Dim sLoginSSO
	Dim sDebug

	' ************************** à modifier dans le cas où on rajoute des items *****************************
	nTypeIncNeSaisPas = 21
	' *******************************************************************************************************
	
	nTypeInc = QuelRadioChecked ("TypeIncident")
	nHardCopy = QuelRadioChecked ("HardCopy")

	sBuf = Trim(document.getElementById("Incident").value)
	If Len(sBuf) < 5 Then
		MsgBox "Veuillez décrire l'incident S.V.P."
		Exit Sub
	End If		
	If nTypeInc = nTypeIncNeSaisPas Then
		nRep =   MsgBox("La sélection d'un type d'incident (autre que 'Ne sais pas')" & vbCrLf & "permet de cibler le correspondant informatique" & vbCrLf &_
				"et donc d'accélérer le processus de résolution." & vbCrLf &_
				"Souhaitez-vous le préciser avant envoi ?",vbYesNo)
