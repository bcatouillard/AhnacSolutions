		sCc = "hotline@ahnac.com"
		sTypeReduit = "Zenidoc"
	Case 17		' Axya
		sTo = "fjasiak@ahnac.com;adelcroix@ahnac.com"
		sCc = "hotline@ahnac.com"
		sTypeReduit = "Axya"
	Case 18		' PACS
		sTo = "mnalepa@ahnac.com"
		sCc = "hotline@ahnac.com"
		sTypeReduit = "PACS"
	Case 19		' Cursus
		sTo = "sbethencourt@ahnac.com;jdanielczyk@ahnac.com"
