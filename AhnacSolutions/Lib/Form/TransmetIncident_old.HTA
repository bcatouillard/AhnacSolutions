	Case 11		' Titan
		sTo = "fjasiak@ahnac.com;dpeers@ahnac.com"
		sCc = "hotline@ahnac.com"
		sTypeReduit = "Titan"
	Case 12		' Générix
		sTo = "bzoks@ahnac.com;cchoquet@ahnac.com"
